#include <stdio.h>
#include <string.h>
#include <ctype.h>
#define MAX_LEN 1000
void printString(char S[], int N) {
    char plaintext[5][MAX_LEN]; 
    int freq[26] = { 0 };  
    int freqSorted[26]; 
    int Used[26] = { 0 };
    int i,j,k;
    for (i = 0; i < N; i++) {
        if (isalpha(S[i])) {
            freq[toupper(S[i]) - 'A']++;
        }
    }
    for (i = 0; i < 26; i++) {
        freqSorted[i] = freq[i];
    }
    char T[] = "ETAOINSHRDLCUMWFGYPBVKJXQZ";
    for (i = 0; i < 26; i++) {
        for (j = i + 1; j < 26; j++) {
            if (freqSorted[i] < freqSorted[j]) {
                int temp = freqSorted[i];
                freqSorted[i] = freqSorted[j];
                freqSorted[j] = temp;
            }
        }
    }
    for (i = 0; i < 5; i++) {
        int ch = -1;
        for (j = 0; j < 26; j++) {
            if (freqSorted[i] == freq[j] && Used[j] == 0) {
                Used[j] = 1;
                ch = j;
                break;
            }
        }
        if (ch == -1)
            break;
        int x = T[i] - 'A' - ch;
        for (k = 0; k < N; k++) {
            if (isalpha(S[k])) {
                int y = toupper(S[k]) - 'A' + x;
                if (y < 0)
                    y += 26;
                if (y >= 26)
                    y -= 26;
                plaintext[i][k] = 'A' + y;
            } else {
                plaintext[i][k] = S[k];
            }
        }
        plaintext[i][N] = '\0'; 
    }
    for (i = 0; i < 5; i++) {
        printf("%s\n", plaintext[i]);
    }
}
int main() {
    char S[] = "B TJNQMF NFTTBHF"; 
    int N = strlen(S); 
    printString(S, N);
    return 0;
}
